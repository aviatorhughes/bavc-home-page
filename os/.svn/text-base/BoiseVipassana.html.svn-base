<!-- josh's script to confirm a valid amount was entered into donation fields - modified to simply check val>0  -->
<script type="text/javascript"> 
function validateAmount(amount){
//	if(amount.value.match( /^[0-9]+(\.([0-9]+))?$/)){
	if(amount.value > 0){
		return true;
	}else{
		alert('You must enter a valid quantity.');
		amount.focus();
		return false;
	}
}

<!-- script from doantions.html for recurring donations - modified to remove 'fundType' code  -->
function setupRecurringValues()
{
  $("input[name='shopping-cart.items.item-1.subscription.payments.subscription-payment-1.maximum-charge']").val($("#recurPrice").val());
  $("input[name='shopping-cart.items.item-1.unit-price']").val($("#recurPrice").val());
  return true;
}
</script>
